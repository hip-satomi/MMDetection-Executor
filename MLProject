name: MMDetection-Executor

conda_env: conda.yaml
# Can have a docker_env instead of a conda_env, e.g.
# docker_env:
#    image:  mlflow-docker-example

entry_points:
  main:
    parameters:
      input_images: path
      config: path
      checkpoint: path
    command: "python main.py --config {config} --checkpoint {checkpoint} {input_images}"

